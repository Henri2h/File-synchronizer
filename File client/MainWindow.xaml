<Window x:Class="File_client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:File_client"
        mc:Ignorable="d"
        Title="MainWindow" Height="527" Width="1099">
    <Grid>
        <TextBox x:Name="tbText" HorizontalAlignment="Left" Height="420" Margin="10,25,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="1010" TextChanged="tbText_TextChanged"/>
        <Button x:Name="btStart" Content="start" HorizontalAlignment="Left" Height="19" Margin="971,467,0,0" VerticalAlignment="Top" Width="110" Click="btStart_Click"/>
        <Button x:Name="btSave" Content="save" HorizontalAlignment="Left" Height="19" Margin="875,467,0,0" VerticalAlignment="Top" Width="91" Click="btSave_Click"/>

    </Grid>
</Window>
